{
  "name": "nekopoi-scraper",
  "version": "2.5.9",
  "description": "Simple nekopoi scraper",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "main": "./dist/cjs/index.js",
  "types": "./dist/esm/index.js",
  "scripts": {
    "postinstall": "npm run build",
    "build": "npm run clean && npm run build:esm && npm run build:cjs && npm run clean:src",
    "build:cjs": "tsc -P tsconfig.cjs.json",
    "build:esm": "tsc -P tsconfig.json",
    "clean": "rm -rf dist",
    "clean:src": "rm -rf src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xct007/nekopoi-scrapper.git"
  },
  "keywords": [
    "nekopoi",
    "lightweight",
    "scraper",
    "api",
    "hentong"
  ],
  "author": "FrierenDv",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xct007/nekopoi-scrapper/issues"
  },
  "homepage": "https://github.com/xct007/nekopoi-scrapper",
  "dependencies": {
    "axios": "^1.1.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "devDependencies": {
    "@types/node": "^18.11.9"
  }
}
